:root {
  --main-font: 'Roboto', sans-serif;
  --secondary-font: 'Raleway', sans-serif;
  --main-color: #2196f3;
  --dark-color: #212121;
  --gray-color: #757575;
  --litegray-color: #afb1b8;
  --white-color: #ffffff;
  --main-section-color: #2f303a;
  --button-color: #f5f4fa;
  --gray-color-transparent: rgba(255, 255, 255, 0.6);
  --shadow-color: rgba(0, 0, 0, 0.15);
}

body {
  background-color: var(--white-color);
  font-family: var(--main-font);
  color: var(--dark-color);
}

h1,
h2,
h3,
h4,
h5,
h6{
  margin: 0;
  margin-block-start: 0;
  margin-block-end: 0;
  padding: 0;
}

p{
  padding: 0;
  margin: 0;
}

ul,
ol {
  margin: 0;
  padding: 0;
}

figure {
  margin: 0;
  padding: 0;
}

img {
  display: block;
}

.hidden {
  position: absolute;
  top: -9999px;
  left: -9999px;
}

.link {
  text-decoration: none;
}

.container {
  width: 1200px;
  padding: 0 15px;
  align-items: center;
  margin: 0 auto;
}

.section {
  padding-top: 94px;
  padding-bottom: 94px;
}

.team-header {
  font-weight: 700;
  font-size: 36px;
  line-height: 1.167;
  /* 42px */
  text-align: center;
  letter-spacing: 0.03em;
  color: var(--dark-color);
}

/* .link:visited,
.link:link */

.navi-list-link {
  color: var(--dark-color);
}

.link {
  /* transition-duration: 3s, 1s;
  transition-property: margin-right, color; */
  /* transition: color 250px 3s, 1s linear; */
  transition: color 250ms linear, transform 250ms ease;
}

.navi-list,
.adres,
.atut-list,
.work-list,
.team-list,
.prace,
.project-list,
.adres-footer-list,
.list {
  list-style: none;
}

.foter {
  background-color: var(--dark-color);
  color: var(--white-color);
}

/* ==== header ==== */
.header .container {
  display: flex;
  align-items: center;
  padding-top: 0;
  padding-bottom: 0;
  height: 80px;
  /* padding-top: 24px;
  padding-bottom: 25px; może to będzie dobrze*/
  margin: 0 auto;
}

.header {
  border-bottom: 1px solid #ECECEC;
  /* border-bottom-width: 1px;
  border-bottom-style: solid;
  border-bottom-color: #ECECEC; */
}

.logo-menu {
  display: block;
  margin-right: 93px;
}

.navi-list-item:not(:last-child) {
  margin-right: 46px;
}

.current-navi .navi-list-link::after {
  position: relative;
  display: block;
  content: "";
  width: 100%;
  height: 4px;
  top: 30px;
  border-radius: 2px;
  background-color: var(--main-color);
}

.mail-link {
  margin-left: auto;
  margin-right: 50px;
}

.navi {
  display: flex;
  align-items: center;
}

.navi-list {
  display: flex;
}

/* --- logo --- */
.logo {
  font-family: 'Raleway', sans-serif;
  font-weight: 700;
  font-size: 26px;
  /* line-size = 1.192 = line-size w px (np. 31px) podzielić przez font-size (np. 26px) */
  line-height: 1.192;
  /* 31px */
  letter-spacing: 0.03em;
  color: var(--dark-color);
}

.logo:visited {
  color: var(--dark-color);
}

.agencja-navi,
.portfolio-navi {
  color: var(--main-color);
}

.logo-web {
  color: var(--main-color);
}

/* nawigacja */

.navi-list,
.adres {
  font-weight: 500;
  font-size: 14px;
  line-height: 1.143;
  /* 16 */
  letter-spacing: 0.02em;
  color: var(--dark-color);
  transition: 250ms cubic-bezier(0.4, 0, 0.2, 1);
}

.navi-list-link:hover,
.navi-list-link:focus,
.adres-mail:hover,
.adres-mail:focus,
.adres-phone:hover,
.adres-phone:focus,
.adres-icon-mail:focus,
.adres-icon-mail:hover,
.adres-icon-phone:focus,
.adres-icon-phone:hover,
.adres-footer-list-item > .link:hover,
.adres-footer-list-item > .link:focus {
  color: var(--main-color);
  fill: var(--main-color);
  /* transition: color 250ms cubic-bezier(0.4, 0, 0.2, 1);
  transition: fill 250ms cubic-bezier(0.4, 0, 0.2, 1); */
}

.adres,
.adres-footer {
  font-style: normal;
}

.adres {
  display: flex;
  align-items: center;
  color: var(--gray-color);
  fill: var(--gray-color);
}

.adres-icon-mail,
.adres-icon-phone {
  display: flex;
  margin-right: 10px;
  /* fill: var(--gray-color); */
}

/* .adres-icon-mail {
  width: 16px;
}

.adres-icon-phone {
  width: 10px;
} */

/* ==== main ===== */
.main-section .container {
  display: block;

  /* width: 1600px; */
  color: var(--white-color);
  /* padding: 0px; */
  /* padding-left: 215px;
  margin: 0 auto;
   */
  padding-top: 200px;
  padding-bottom: 200px;
}

.main-section {
  display: block;
  background-image: linear-gradient(
    rgba(47, 48, 58, 0.4), 
    rgba(47, 48, 58, 0.4)), 
    url("../images/main/main-bg.jpg");
  background-size: cover;
  background-repeat: no-repeat;
  /* background: var(--main-section-color); */
  /* padding-top: 200px; */
  /* padding-top: 0; */
  /* padding-bottom: 200px; */
}

.main-header {
  font-weight: 900;
  font-size: 44px;
  line-height: 1.363;
  /* 60px */
  letter-spacing: 0.06em;
  text-align: center;
  justify-content: center;
  /* justify-items: center; */
  /* color: var(--white-color); */
  width: 696px;

  display: block;
  align-items: center;
  /* padding-top: 200px; */
  margin-left: auto;
  margin-right: auto;
  margin-bottom: 30px;
}

.button-main {
  font-weight: 700;
  font-size: 16px;
  line-height: 1.875;
  /* 30px */
  letter-spacing: 0.06em;
  color: var(--dark-color);
  width: 210px;
  height: 50px;

  cursor: pointer;
  border-style: none;
  border-radius: 4px;

  display:block;
  box-shadow: 0px 4px 4px var(--shadow-color);
  align-items: center;
  text-align: center;
  padding: 10px 41px 10px 42px;
  margin-left: auto;
  margin-right: auto;
  /* margin-bottom: 200px; */

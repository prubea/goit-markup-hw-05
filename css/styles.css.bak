/* ====== portfolio - projects ======= */
/* ---- button ---- */
.button-portfolio-section .container {
  display:block;
}

.button-portfolio-section {
  display: flex;
  padding-bottom: 0;
  /* margin: 94px 0; */
}

.prace {
  margin-bottom: 50px;
  display: flex;
  justify-content: center;
}

.button-portfolio {
  text-align: center;
  border-style: none;
  border-radius: 4px;
  padding: 6px 22px;
}

.buton-item-portfolio:not(:last-child) {
  margin-right: 8px;
}

/* ---- projects ---- */
.project .container {
  display: flex;
}

.project {
  padding-top: 0;
  margin: 0;
}

.project-list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.project-list-item {
  flex-basis: calc(100% / 3 - 30px);
  /* padding-bottom: 20px; */
  margin-bottom: 30px;
}

.team-list-item:focus,
.team-list-item:hover,
.project-list-item:focus,
.project-list-item:hover {
  box-sizing: border-box;
  box-shadow: 0px 1px 1px rgba(0, 0, 0, 0.12),
  0px 4px 4px rgba(0, 0, 0, 0.06),
  1px 4px 6px rgba(0, 0, 0, 0.16);
  transition: color 250ms cubic-bezier(0.4, 0, 0.2, 1);
}

.img-project {
  width: 370px;
  height: 294px;
}

.img-figure:hover .overlay-portfolio {
  transform: translatey(0);
  transition: color 250ms cubic-bezier(0.4, 0, 0.2, 1);
}

.img-figure {
  position: relative;
  cursor: pointer;
  overflow: hidden;
  transition: 250ms cubic-bezier(0.4, 0, 0.2, 1);
}

.img-figure .overlay-portfolio {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 294px;
  background-color: rgba(33, 150, 243, 0.9);

  transform: translatey(150%);
  transition: transform 250ms cubic-bezier(0.4, 0, 0.2, 1);
      
  font-weight: 400;
  font-size: 18px;
  line-height: 1.556;
  /* or 28px */
  letter-spacing: 0.03em;
  color: var(--white-color);
  padding: 49px 45px 49px 24px;
  margin: 0;
}

.img-podpis {
  display: block;
  box-sizing: border-box;
  padding: 20px 24px;
  border: 1px solid #ECECEC;
  border-top: 0;
}

.header-project {
  width: 322px;
  font-weight: 700;
  font-size: 18px;
  line-height: 2;
  /* 36px */
  letter-spacing: 0.06em;
  color: var(--dark-color);
}

.p-project {
  font-weight: 400;
  font-size: 16px;
  line-height: 1.875;
  /* 30px */
  letter-spacing: 0.03em;
  color: var(--gray-color);
}

/* ====== footer ====== */
.foter .container {
  display: flex;
  /* width: 1600px; */
  color: var(--white-color);
  padding: 60px 0;
  /* padding-left: 215px; */
  text-align: left;
  margin: 0 auto;
}

.adres-footer {
  font-weight: 400;
  font-size: 14px;
  line-height: 1.714;
  /* 24px */
  letter-spacing: 0.03em;
  display:flex;
}

.adres-footer-list-item:not(:last-child) {
  padding-bottom: 9px;
}

.foter-left > .logo-dark {
  color: var(--white-color);
  display: block;
  padding-bottom: 20px;
}

.adres-footer-list-item > .adres-mail,
.adres-footer-list-item > .adres-phone {
  color: var(--gray-color-transparent);
  transition: color 250ms cubic-bezier(0.4, 0, 0.2, 1);
}

.container .foter-right {
  display: block;
  /* align-items: flex-start; */
  align-self: flex-start;
  justify-content: space-between;
  margin-left: 70px;
  flex-wrap: wrap;
}

.foter-p {
  display: flex;
  font-weight: 700;
  font-size: 14px;
  line-height: 1.188;
  /* 19px */
  /* text-align: center; */
  letter-spacing: 0.03em;
  color: var(--white-color);
  margin-top: 12px;
  margin-bottom: 20px;
}

.foter-social-list {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 16px;
}

.foter-social-list-link {
  display: flex;
  width: 44px;
  height: 44px;
  background-color: rgba(255, 255, 255, 0.1);
  border-radius: 50%;
  align-items: center;
  justify-content: center;
  fill: var(--white-color);
}

.foter-social-list-item:not(:last-child) {
  margin-right: 10px;
}

.foter-social-list-link:focus,
.foter-social-list-link:hover {
  background-color: var(--main-color);
  transition: background-color 250ms cubic-bezier(0.4, 0, 0.2, 1);
  transition: fill 250ms cubic-bezier(0.4, 0, 0.2, 1);
}

/* .foter-social-icon {
  width: 20px;
  height: 20px;
} */

/* ==== Okno modalne - backdrop ====*/
.backdrop {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.1);
  box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.12),
  0px 1px 1px rgba(0, 0, 0, 0.14),
  0px 2px 1px rgba(0, 0, 0, 0.2);
  border-radius: 4px;
  transition: 250ms cubic-bezier(0.05, 0.7, 0.8, 0.5);
  /* transform: scale(1);
  animation: roadRunnerIn 0.3s cubic-bezier(0.165, 0.84, 0.44, 1) forwards; */
  cursor: pointer;
}

/* .backdrop.out {
  animation: quickScaleDown 0s .5s linear forwards;
  animation: roadRunnerOut 0.5s cubic-bezier(0.165, 0.84, 0.44, 1) forwards;
} */

.modal {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  /* mogą być inne wymiary okna modalnego: */
  width: 528px;
  min-height: 581px;
  background-color: var(--white-color);
  cursor: pointer;
}

.iksik {
  fill: #000;
  margin-top: 30%;
}

.close-btn {
  position: absolute;
  top: 8px;
  right: 8px;
  width: 30px;
  height: 30px;
  fill: #000;
  background-color: transparent;
  border: 1px solid rgba(0, 0, 0, 0.1);
  border-radius: 50%;
  cursor: pointer;
}

.is-hidden {
  opacity: 0;
  visibility: hidden;
  pointer-events: none;
}
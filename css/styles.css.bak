  display:block;
}

.atut {
  padding-bottom: 0;
}

.atut-list {
  display: flex;
  /* justify-content: space-evenly; */
  flex-wrap: wrap;
  justify-content: space-between;
}

/* .atut-list-item:not(:last-child) {
  margin-right: 30px;
}   */

.atut-list-item {
  flex-basis: calc(100% / 4 - 30px);
  /* padding-bottom: 20px; */
  margin-bottom: 30px;
  align-items: center;
  justify-content: center;
}


.atut-social-icon {
  display: flex;
  /* width: 70px;
  height: 70px;   */
}

.atut-l-icon {
  display: flex;
  align-items: center;
  width: 270px;
  height: 120px;
  background: #F5F4FA;
  border-radius: 4px;
}

.atut-header {
  display: flex;
  width: 270px;
  font-weight: 700;
  font-size: 14px;
  line-height: 1.143;
  /* 16px */

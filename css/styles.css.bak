  color: var(--dark-color);
}

.atut-p:not(:last-child) {
  margin-right: 30px;
}

/* Work - main page */

.work-section .container {
  display: block;
  /* justify-content: space-evenly; */
  /* padding-bottom: 94px; */
}

.work-header,
.team-header,
.klient-header {
  font-family: var(--main-font);
  font-weight: 700;
  font-size: 36px;
  line-height: 1.167;
  /* 42px */
  text-align: center;
  letter-spacing: 0.03em;
  color: var(--dark-color);
  margin-bottom: 50px;
}

.work-list {
  display: flex;
}

.work-list-item:not(:last-child) {
  margin-right: 30px;
}

.work-list-item {
  position: relative;
}

.work-list-item>.label {
  position: absolute;
  bottom: 0;
  /* left: 0; */
  margin: 0;
  padding: 27px 0;
  width:370px;
  
  font-family: var(--main-font);
  color: var(--white-color);
  font-weight: 700;
  font-size: 14px;
  line-height: 1.143;
  /* 16px */
  text-align: center;
  letter-spacing: 0.03em;
  text-transform: uppercase;
  background-color: rgba(47, 48, 58, 0.8);
}

/* zespół team - main page */
.team .container {
  display: block;
  /* justify-content: space-evenly; */
  /* padding-top: 94px;
  padding-bottom: 94px; */
}

.team {
  background-color: var(--button-color);
}

.team-opis {
  padding: 30px 0;
  /* padding-bottom: 30px; */
}

.team-list {
  display: flex;
}

.team-list-item {
  background: var(--white-color);
    
  box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.12),
  0px 1px 1px rgba(0, 0, 0, 0.14),
  0px 2px 1px rgba(0, 0, 0, 0.2);
  border-radius: 0px 0px 4px 4px;
}

.team-list-item:not(:last-child) {
  margin-right: 30px;
}

.team-header-name {
  font-weight: 500;
  font-size: 16px;
  line-height: 1.188;
  /* 19px */
  text-align: center;
  letter-spacing: 0.03em;
  color: var(--dark-color);
}

.team-p {
  font-weight: 400;
  font-size: 16px;
  line-height: 1.188;
  /* 19px */
  text-align: center;
  letter-spacing: 0.03em;
  color: var(--gray-color);
}

.team-social-list {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 16px;
}

.team-social-list-link {
  display: flex;
  width: 44px;
  height: 44px;
  background-color: var(--white-color);
  border-radius: 50%;
  align-items: center;
  justify-content: center;
  fill: var(--litegray-color);
  transition: 250ms cubic-bezier(0.4, 0, 0.2, 1);
}

.team-social-list-link:not(:last-child) {
  margin-right: 10px;
}

.team-social-list-link:focus,
.team-social-list-link:hover {
  background-color: var(--main-color);
  fill: var(--white-color);
  /* transition: background-color 250ms cubic-bezier(0.4, 0, 0.2, 1);
  transition: fill 250ms cubic-bezier(0.4, 0, 0.2, 1); */
}

/* .team-social-icon {
  width: 20px;
  height: 20px;
} */

/* ---- Klienci ---- */
.klient .container {
  display: block;
}

.klient {
  padding-bottom: 0;
}

.klient-list {
  display: flex;
  /* justify-content: space-evenly; */
  flex-wrap: wrap;
  justify-content: space-between;  
}

.klient-list-item:not(:last-child) {
  margin-right: 30px;
}  

/* .klient-list-item */
.klient-social-list-link {
  /* flex-basis: calc(100% / 4 - 30px); */
  /* padding-bottom: 20px; */
